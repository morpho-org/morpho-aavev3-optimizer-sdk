{
  "name": "@morpho-org/morpho-aave-v3-sdk",
  "version": "1.0.0",
  "description": "ðŸ“¦ SDK library for interacting with Morpho AaveV3",
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "lint:fix": "eslint ./src ./tests --fix && prettier -w ./src ./tests",
    "lint": "eslint ./src ./tests && prettier -c ./src ./tests",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test": "jest --coverage --detectOpenHandles",
    "test:e2e": "hardhat test --tsconfig ./tsconfig.test.e2e.json --v",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morpho-org/morpho-aave-v3-sdk.git"
  },
  "license": "MIT",
  "author": "Morpho Association",
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "ethers",
    "lending",
    "sdk",
    "morpho",
    "aave",
    "evm",
    "smart contract",
    "web3"
  ],
  "bugs": {
    "url": "https://github.com/morpho-org/morpho-aave-v3-sdk/issues"
  },
  "homepage": "https://github.com/morpho-org/morpho-aave-v3-sdk#readme",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/networks": "^5.7.1",
    "@ethersproject/providers": "^5.7.2",
    "@gnosis.pm/safe-apps-sdk": "^7.8.0",
    "@morpho-labs/ethers-utils": "^1.2.0",
    "@morpho-labs/gnosis-tx-builder": "^1.3.1",
    "@morpho-labs/morpho-ethers-contract": "^1.19.0",
    "ethers": "^5.7.2",
    "ethers-multicall-provider": "^3.0.3",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "ethers": "^5.7.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.4",
    "@types/sinon": "^10.0.14",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-scope": "^7.2.0",
    "hardhat": "^2.14.0",
    "hardhat-deal": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^2.3.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sinon": "^15.0.4",
    "solidity-coverage": "^0.8.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "verbose": true,
    "testTimeout": 15000,
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "testRegex": "(/tests/units/.*|(\\.|/)(test|spec)+)\\.test\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "./tests/units/jest.matchers.ts"
    ],
    "preset": "ts-jest"
  }
}
